--Main
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("BP Hub", "Midnight")
local plr = game:service('Players')

--Info
local Info = Window:NewTab("Info")
local InfoSection = Info:NewSection("Created by bpitcher2013#7057")
InfoSection:NewLabel("Version 2.3")
InfoSection:NewKeybind("Close/Open UI", "Opens And Closes The Gui", Enum.KeyCode.P, function()
    Library:ToggleUI()
end)

local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Aimbot", "Aims For You", function()
loadstring(game:HttpGet('https://pastebin.com/raw/HaRE3BbZ'))()
end)

MainSection:NewButton("ESP", "Shows Players Nametags Through Walls", function()
loadstring(game:HttpGet('https://pastebin.com/raw/xcdn4qKA'))()
end)

MainSection:NewButton("Silent Aim", "Makes Bullets Atrract Closer To Eachother", function()
loadstring(game:HttpGet('https://pastebin.com/raw/Qfuegsu4'))()
end)
